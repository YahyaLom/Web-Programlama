@model Polikinlik
<h2>Polikinlik Sil Sayfasi</h2>

<form method="post">

	

	<div class="border p-3 mt-5">

		<div class="row pb-3  form-group">
			<h2 class="text-primary "><strong>Polikinlik Sil</strong> </h2>
			</hr>
		</div>


		<div class="mb-3  form-group"
		<label asp-for="Ad"></label>
		<input asp-for="Ad" disabled class="form-control">

		</div>

		<div class="mb-3  form-group"
		<label asp-for=Adres></label>
		<input asp-for="Adres" disabled class="form-control">

		</div>

		<br />

		<button type="submit" class="btn btn-outline-primary" style="width:250px">Polikinlik Sil</button>

		<a asp-controller="Polikinlik" asp-action="Index" class="btn btn-outline-primary style="width:250px">
			Polikinlik Listesine Dön
		</a>

	</div>






</form>